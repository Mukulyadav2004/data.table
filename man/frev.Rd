\name{frev}
\alias{frev}
\alias{rev}
\title{Fast reverse}
\description{
  Similar to \code{\link[base]{rev}} but \emph{much faster}.
}

\usage{
frev(x, copy=TRUE)
}
\arguments{
  \item{x}{ An atomic \code{vector} or \code{list}. }
  \item{copy}{ logical (default is \code{TRUE}). If \code{FALSE} reversing happens in-place. }
}

\value{
  \code{frev} returns the input reversed.
}

\examples{
# on vectors
x = setNames(1:26, letters)
frev(x[1:10])
# reverse in-place
frev(x, copy=FALSE)
x

# list
frev(list(1, "a"))
}
\keyword{ data }
